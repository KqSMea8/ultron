    "prestart": "npm run tslint:development",
    "start": "cross-env IS_DEV=1 NODE_ENV=development PORT=8090 webpack-dev-server --progress --colors --config webpack.config.babel.js",
    "start:no-proxy": "npm run tslint:development && cross-env NODE_ENV=development PORT=8090 NO_PROXY=true webpack-dev-server --progress --colors --config webpack.config.babel.js",
    "build": "npm run tslint:production && cross-env NODE_ENV=production webpack --progress --colors --config webpack.config.babel.js",
    "build:pre": "npm run tslint:production && cross-env NODE_ENV=production PRE=true webpack --progress --colors --config webpack.config.babel.js",
    "tslint:development": "tslint -p . -c tslint.json",
    "tslint:production": "tslint -p . -c tslint.pro.json",
    "dev": "sh shell/dev_proxy.sh",
    "mock": "sh shell/dev_mock.sh"
  },
  "pre-commit": [
    "tslint:production"
